				<a href="/" class="home"><span class="blind">홈</span></a>
				<div class="depth">
                    <a href="#none"><span>이용안내</span></a>                    
					<div class="sub_list">
						<ul>
                            <li><a href="@Url.Action("greeting", "company")"><span>회사소개</span></a></li>
                            <li><a href="@Url.Action(" service", "Service" )"><span>서비스소개</span></a></li>
							<li><a href="@Url.Action(" partner", "Partner" )"><span>파트너</span></a></li>
							<li><a href="@Url.Action("notice", "community")"><span>게시판</span></a></li>
						</ul>
					</div>
				</div>

				<div class="depth now">
                    @{  
                        string subTitle = ViewBag.Sub;
                        switch (subTitle)
                        {
                            case "policy":<a href="#none"><span>개인정보처리방침</span></a>break;
                            case "terms":<a href="#none"><span>이용약관</span></a>break;
                        }                        
					    <div class="sub_list">
						    <ul>  
                                @{ if (ViewBag.Sub != "policy"){<li><a href="@Url.Action("policy", "page")"><span>개인정보처리방침</span></a></li>}}
                                @{ if (ViewBag.Sub != "terms"){<li><a href="@Url.Action("terms", "page")"><span>이용약관</span></a></li>}}                                        
						    </ul>
					    </div>
                    }
				</div>
			</div>
		</div>
		<!-- //location -->

		<!-- container -->
		@RenderSection("container", required: false)
		<!-- //container -->

