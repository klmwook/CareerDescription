                                        <tr>
                                            <th>구분</th>
                                            <th>파일명</th>
                                        </tr>
                                    </thead>
                                </table>
                                <div class="scrollbar_bkfile">
                                    <table class="list_type1 no_border keep_on">
                                        <colgroup>
                                            <col class="w8" />
                                            <col />
                                        </colgroup>
                                        <tbody>
                                            <tr>
                                                <td>
                                                    <select class="select" id="select_FileList_FileSeparation" name="select_FileList_FileSeparation">
