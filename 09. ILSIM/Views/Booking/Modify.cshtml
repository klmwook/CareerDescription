                                    <input type="file" id="upload_btn" class="upload_btn" multiple="multiple">
                                </div>
                                <!-- //file_box -->
                                <table class="list_type1 keep_on">
                                    <colgroup>
                                        <col class="w8" />
                                        <col />
                                    </colgroup>
