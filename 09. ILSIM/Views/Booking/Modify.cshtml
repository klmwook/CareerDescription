        </div>
        <div class="sub_cover" id="SubCover">
            <div class="fixed_in type2">
                <!-- booking_write -->
                <div class="booking_write">
                    <!-- left_area -->
                    <div class="left_area">
                        <div class="bk_box">
                            <table class="list_type1">
                                <colgroup>
                                    <col class="w3">
                                    <col>
                                    <col class="w13">
                                    <col class="w13">
                                    <col class="w13">
                                    <col class="w13">
                                </colgroup>
                                <thead>
                                    <tr>
                                        <th colspan="2">Carrier</th>
                                        <th>Service</th>
                                        <th>ETD</th>
                                        <th>ETA</th>
                                        <th>D/Closing</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><img src="~/Images/logo/KMTC.png" /></td>
                                        <td>KMTC ULSAN, 2022S<br />3Days, Direct</td>
                                        <td>FCL</td>
                                        <td>2021.06.10<br />18:00</td>
                                        <td>2021.06.17<br />18:00</td>
                                        <td>2021.06.10<br />18:00</td>
                                        <td class="mobile_layout" colspan="6">
                                            <div class="layout_type2">
                                                <div class="row s2">KMTC ULSAN, 2022S</div>
                                                <div class="row s3">
                                                    <table>
                                                        <tbody>
                                                            <tr>
                                                                <th>Carrier</th>
                                                                <td><img src="/Images/logo/KMTC.png" alt=""></td>
                                                            </tr>
                                                            <tr>
                                                                <th>Service</th>
                                                                <td>FCL</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Departure</th>
                                                                <td>2020.10.25(일) KRPUS</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Arrival</th>
                                                                <td>2020.10.30(금) HKHKG</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Doc Closing</th>
